<h1>Admin</h1>


<table class="table">
    <thead>
      <tr>

        <th scope="col">Nombre</th>
        <th scope="col">Email</th>
      </tr>
    </thead>
    <tbody>
        <% @users.each do |user| %>
            <tr>
               
                <td><%= user.name %></td>
                <td><%= user.email %></td>
                <td><%= link_to "Edit", edit_user_path(user) %></td>
              
            </tr>
        <% end %>
    </tbody>
  </table>